# Ignore Ruby environment files
/.bundle/
/vendor/bundle
/.env

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore system files
*.swp
*~
.DS_Store

# Ignore RubyMine project files
.idea/

# Ignore temporary files
/tmp

# Ignore the node_modules directory
/node_modules/

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore files related to testing
/coverage/
/spec/tmp/

# Ignore Byebug command history file.
.byebug_history

# Ignore configuration files that are typically generated by utilities.
/config/master.key
/credentials.yml.enc
